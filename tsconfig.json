{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceRoot": "src",
    "outDir": "dist",
    "sourceMap": true
  },
  "exclude": [
    "node_modules"
  ]
}
